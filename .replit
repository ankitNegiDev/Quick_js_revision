entrypoint = "index.js"
modules = ["nodejs-22"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"

[deployment]
run = ["node", "index.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[workflows]
runButton = "prototypes"

[[workflows.workflow]]
name = "loop"
author = 23902305
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node Basic_js/loops.js"

